/**index.wxss**/

.swiper-item {
  width: 100%;
  height: 300rpx;
}

.index-nav {
  display: flex;
  flex-flow:wrap;
}

.index-nav .nav-item  {
  display: flex;
  width: 33.333%;
  height: 200rpx;
  box-sizing: border-box;
  /* border: 1px solid ghostwhite; */
  border-bottom: 1px solid ghostwhite;
  border-right: 1px solid ghostwhite;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-family:  Geneva, Verdana, sans-serif ;
}
.nav-item:nth-child(3n){
  border-right: 0 none;
}
.index-nav .nav-item image{
  width: 102rpx;
  height: 102rpx;
}

.index-enter{
  display: flex;
  padding: 20rpx 42rpx;
}
.index-enter .enter-item{
   width: 50%;
 display: flex;
  justify-content: center;
}

.index-enter .enter-item image{
  width: 320rpx;
  height: 178rpx;
}